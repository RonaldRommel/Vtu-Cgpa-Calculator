<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs")%>
  <body>
    <div class="container">
      <div class="cgpa_calculator align-items-center">
        <center>
          <h2><i><%= sem%></i></h2>
          <br />
          <% subjects.forEach(sub=>{%><label for="c1s1"><%=sub%></label>
          <input type="number" id="s3" max="100" min="0" /><br /><br /><% }) %>
          <button class="btn btn-sm btn-outline-primary">Calculate</button>
          <input
            type="reset"
            class="btn btn-sm btn-outline-danger"
          /><br /><br />
          <div class="row justify-content-center">
            <div class="col-md-3 mt-2">
              <label for="c1cgpa" class="c1cgpa">CGPA</label>
              <input type="text" id="c1cgpa" />
            </div>
            <div class="col-md-3 mt-2">
              <label for="c1per" class="c1per">Percentage%</label>
              <input type="text" id="c1per" />
            </div>
          </div>
        </center>
      </div>
    </div>
  </body>
</html>
